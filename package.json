{
  "name": "control-flow-graph",
  "version": "0.1.1",
  "description": "A control flow graph display in browser",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf ./lib && webpack --config webpack.build.config.js",
    "prepublish": "npm run build && npm version patch",
    "start": "webpack-dev-server --host 0.0.0.0 --progress --profile --colors --inline --hot --port 8888"
  },
  "keywords": [
    "control flow",
    "graph"
  ],
  "browser": {
    "fs": false,
    "path": false,
    "crypto": false
  },
  "author": "liyinan",
  "license": "MIT",
  "dependencies": {
    "d3": "^4.2.2",
    "d3-measure-text": "0.0.2",
    "d3-sankey": "^0.7.1",
    "jquery": "^3.2.1",
    "lodash": "^4.15.0",
    "svg-pan-zoom": "^3.5.0",
    "viz.js": "^1.7.1"
  },
  "peerDependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.9.1",
